  </div>
  <div class="field">
   
    <%= f.label :shoppingOrder_id %><br>
    <%= f.number_field :shoppingOrder_id , :class => "form-control"%>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
